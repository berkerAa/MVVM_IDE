<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/reset_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserratmedium"
        android:text="@string/reset"
        android:textColor="@color/button_color"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edit_text_password_resetpassword_screen"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.249" />

    <EditText
        android:id="@+id/edit_text_password_resetpassword_screen"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/custom_input"
        android:ems="10"
        android:gravity="bottom"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reset_password"
        app:layout_constraintVertical_bias="0.105" />

    <EditText
        android:id="@+id/edit_Text_confirm_password"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="116dp"
        android:background="@drawable/custom_input"
        android:ems="10"
        android:gravity="bottom"
        android:inputType="textPassword"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_password_resetpassword_screen"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/reset_password_button_ac_user_resetpassword_layout"
        app:backgroundTint="@null"
        android:layout_width="335dp"
        android:layout_height="47dp"
        android:background="@drawable/custom_input"
        android:backgroundTint="@color/button_color"
        android:fontFamily="@font/montserratmedium"
        android:text="@string/reset"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_Text_confirm_password"
        app:layout_constraintVertical_bias="0.412" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="97dp"
        android:layout_height="23.4dp"
        app:layout_constraintBottom_toTopOf="@+id/reset_password"
        app:layout_constraintEnd_toEndOf="@+id/reset_password"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/reset_password"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.514"
        app:srcCompat="@drawable/page_3" />

    <Button
        android:id="@+id/button_activity_user_changepassword_layout_back_button"
        android:layout_width="10dp"
        android:layout_height="23.4dp"
        android:background="@android:color/transparent"
        android:hint=""
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7_activity_user_resetpassword_layout_popup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat"
        android:lineSpacingExtra="3sp"
        android:text="@string/tw_ac_us_changepassword_popup_message"
        android:textColor="#002e6d"
        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/reset_password_button_ac_user_resetpassword_layout"
        app:layout_constraintEnd_toEndOf="@+id/reset_password_button_ac_user_resetpassword_layout"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edit_Text_confirm_password"
        app:layout_constraintTop_toBottomOf="@+id/edit_Text_confirm_password"
        app:layout_constraintVertical_bias="0.236" />

    <ImageButton
        android:id="@+id/imageButton_ac_user_resetpassword_layout_eye"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/edit_text_password_resetpassword_screen"
        app:layout_constraintEnd_toEndOf="@+id/edit_text_password_resetpassword_screen"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="@+id/edit_text_password_resetpassword_screen"
        app:layout_constraintTop_toTopOf="@+id/edit_text_password_resetpassword_screen"
        app:layout_constraintVertical_bias="0.56"
        app:srcCompat="@drawable/group_15721" />


    <ImageButton
        android:id="@+id/imageButton_ac_user_resetpassword_layout_eye_2"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:backgroundTint="@android:color/transparent"

        app:layout_constraintBottom_toBottomOf="@+id/edit_Text_confirm_password"
        app:layout_constraintEnd_toEndOf="@+id/edit_Text_confirm_password"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="@+id/edit_Text_confirm_password"
        app:layout_constraintTop_toTopOf="@+id/edit_Text_confirm_password"
        app:layout_constraintVertical_bias="0.52"
        app:srcCompat="@drawable/group_15721" />


</androidx.constraintlayout.widget.ConstraintLayout>