<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >


    <ImageView
        android:id="@+id/deportak_logo"
        android:layout_width="197dp"
        android:layout_height="40dp"
        android:src="@drawable/icon_depo_ortak_enter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.068" />

    <EditText
        android:id="@+id/editTextTextEmailAddress_register_screen"
        android:layout_width="330dp"
        android:layout_height="41dp"
        android:background="@drawable/custom_input"
        android:drawableLeft="@drawable/group_15738"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:paddingLeft="10dp"
        android:hint="E-posta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.371" />

    <EditText
        android:id="@+id/editTextPhone_register_screen"
        android:layout_width="202dp"
        android:layout_height="41dp"
        android:background="@drawable/custom_input"
        android:drawableLeft="@drawable/group_1031"
        android:ems="10"
        android:inputType="phone"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.803"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.475" />

    <Spinner
        android:layout_width="112dp"
        android:layout_height="41dp"
        android:background="@drawable/custom_input"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.133"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.475" />

    <EditText
        android:id="@+id/editTextTextPassword_register_screen"
        android:layout_width="330dp"
        android:layout_height="41dp"
        android:background="@drawable/custom_input"
        android:drawableLeft="@drawable/group_1032"
        android:ems="10"
        android:inputType="textPassword"
        android:paddingLeft="10dp"
        android:hint="Şifre"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone_register_screen"
        app:layout_constraintVertical_bias="0.084" />

    <EditText
        android:id="@+id/editTextTextPassword2_register"
        android:layout_width="330dp"
        android:layout_height="41dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/custom_input"
        android:drawableLeft="@drawable/group_1032"
        android:ems="10"
        android:hint="Şifre Tekrar"
        android:inputType="textPassword"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toTopOf="@+id/button_register_screen_register"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword_register_screen"
        app:layout_constraintVertical_bias="0.153" />

    <EditText
        android:id="@+id/editTextTextPersonName_register_screen"
        android:layout_width="160dp"
        android:layout_height="41dp"
        android:layout_marginStart="24dp"
        android:background="@drawable/custom_input"
        android:drawableStart="@drawable/symbol_6_1"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="10dp"
        android:hint="İsim"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress_register_screen"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName2_surname_register_screen"
        app:layout_constraintHorizontal_bias="0.615"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832" />

    <EditText
        android:id="@+id/editTextTextPersonName2_surname_register_screen"
        android:layout_width="160dp"
        android:layout_height="41dp"
        android:background="@drawable/custom_input"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Soyisim"
        android:paddingStart="10dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress_register_screen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.836"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832" />

    <Button
        android:id="@+id/button_register_screen_register"
        android:layout_width="335dp"
        android:layout_height="47dp"
        app:backgroundTint="@null"
        android:background="@drawable/custom_input"
        android:backgroundTint="@color/button_color"
        android:text="@string/textview_register_screen_login"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:fontFamily="@font/montserratmedium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.809"
        app:srcCompat="@drawable/group_1009" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/montserratmedium"
        android:lineSpacingExtra="-3sp"
        android:textColor="#002e6d"
        android:textSize="21sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress_register_screen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/deportak_logo"
        app:layout_constraintVertical_bias="0.0"
        tools:text="@string/textview_register_screen_login" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="4sp"
        android:textColor="#002e6d"
        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_register_screen_register"
        app:layout_constraintVertical_bias="0.252"
        tools:text="@string/tw_ac_us_register_layout_kvk_info_message" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:fontFamily="@font/montserrat"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="11sp"
        android:text="Zaten bir hesabın var mı?"
        android:textColor="#002e6d"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.437" />

    <Button
        android:id="@+id/button_register_screen_login"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.448" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/montserratmedium"
        android:text="@string/textview_register_screen_sign_up"
        android:textColor="#002e6d"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/button_register_screen_login"
        app:layout_constraintEnd_toEndOf="@+id/button_register_screen_login"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat"
        android:lineSpacingExtra="5sp"
        android:text="@string/textview_popup_messagge_this_area_cannot_be_null"
        android:textColor="#002e6d"
        android:textSize="10sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress_register_screen"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPersonName2_surname_register_screen"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName_register_screen"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName_register_screen"
        app:layout_constraintVertical_bias="0.391" />

</androidx.constraintlayout.widget.ConstraintLayout>